  "scripts": {
    "compile": "hardhat compile",
    "verify": "hardhat run ./scripts/verify.ts",
    "deploy": "hardhat deploy-zksync --script deploy.ts --network zksync",
    "deploy:vault": "hardhat deploy-zksync --script deployVault.ts --network zksync",
